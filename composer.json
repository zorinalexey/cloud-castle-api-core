    "guzzle/http": "*",
    "symfony/var-dumper": "*",
    "ramsey/uuid": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ext-fileinfo": "*",
    "ext-simplexml": "*",
    "ext-calendar": "*"
